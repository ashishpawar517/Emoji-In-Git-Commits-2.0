{
    "name": "emojis",
    "displayName": "Emoji-In-Git-Commits-2.0",
    "description": " Use Emojis in Git Commit Messages ",
    "version": "1.0.1",
    "author": {
        "name": "Aashish",
        "email": "pawarashish564@gmail.com"
    },
    "publisher": "Aashish",
    "repository": {
        "type": "git",
        "url": "https://github.com/pawarashish564/Emoji-In-Git-Commits-2.0"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "galleryBanner": {
        "color": "#fad000"
    },
    "keywords": [
        "Emoji In Commits",
        "Emoji",
        "git",
        "emoji",
        "commit",
        "emoji",
        "git emoji"
    ],
    "categories": [
        "Other"
    ],
    "icon": "images/Logo.png",
    "activationEvents": [
        "onCommand:extension.EmojiLog"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.EmojiLog",
                "title": "Emoji-In-Git-Commits-2.0",
                "icon": {
                    "dark": "images/Logo.png",
                    "light": "images/Logo.png"
                }
            }
        ],
        "menus": {
            "scm/title": [
                {
                    "when": "scmProvider == git",
                    "command": "extension.EmojiLog",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "tslint": "^5.20.0"
    }
}
