{
    "name": "emoji-log-vscode",
    "displayName": "Emoji Log",
    "description": "Emojis for Git Commits",
    "version": "2.0.2",
    "author": {
        "name": "Aashish Pawar",
        "email": "pawarashish564@gmail.com"
    },
    "publisher": "Aashish",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pawarashish564/Emoji-In-Git-Commits-2.0"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "galleryBanner": {
        "color": "#fad000"
    },
    "keywords": [
        "Emoji Log",
        "Emojis",
        "git",
        "emoji",
        "commit",
        "messages"
    ],
    "categories": [
        "Other"
    ],
    "icon": "images/icon.png",
    "activationEvents": [
        "onCommand:extension.EmojiLog"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "extension.EmojiLog",
            "title": "Emoji Log ",
            "icon": {
                "dark": "images/icon_dark.svg",
                "light": "images/icon_light.svg"
            }
        }],
        "menus": {
            "scm/title": [{
                "when": "scmProvider == git",
                "command": "extension.EmojiLog",
                "group": "navigation"
            }]
        }
    },
    "badges": [{

        "description": "Support this project!"
    }],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "tslint": "^5.20.0"
    }
}